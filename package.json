{
  "name": "nmtran",
  "displayName": "nmtran",
  "description": "Syntax highlighting for NMTRAN",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "nmtran",
        "aliases": [
          "NMTRAN",
          "nmtran"
        ],
        "extensions": [
          ".mod",
          ".ctl",
          ".lst",
          ".modt",
          ".phi",
          ".coi",
          ".cor",
          ".cov",
          ".cnv",
          ".scm",
          ".ext"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "nmtran",
        "scopeName": "source.nmtran",
        "path": "./syntaxes/nmtran.tmLanguage.json"
      }
    ]
  },
  "devDependencies": {
    "js-yaml": "^4.1.0"
  }
}
