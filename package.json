{
  "name": "nmtran",
  "displayName": "NMTRAN",
  "description": "Syntax highlighting for NMTRAN (NONMEM) files",
  "version": "0.1.3",
  "publisher": "vrognas",
  "license": "MIT",
  "categories": [
    "Programming Languages",
    "Snippets"
  ],
  "icon": "images/nonmem.png",
  "homepage": "https://github.com/vrognas/vscode-nmtran",
  "bugs": {
    "url": "https://github.com/vrognas/vscode-nmtran/issues",
    "email": "viktor@vrognas.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vrognas/vscode-nmtran.git"
  },
  "activationEvents": [
    "onLanguage:nmtran"
],
  "main": "./out/extension.js",
  "engines": {
    "vscode": "^1.80.0"
  },
  "contributes": {
    "languages": [
      {
        "id": "nmtran",
        "aliases": [
          "NMTRAN",
          "nmtran"
        ],
        "extensions": [
          ".mod",
          ".ctl",
          ".lst",
          ".modt",
          ".phi",
          ".coi",
          ".cor",
          ".cov",
          ".cnv",
          ".scm",
          ".ext"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "nmtran",
        "scopeName": "source.nmtran",
        "path": "./syntaxes/nmtran.tmLanguage.json"
      }
    ],
    "snippets": [
      {
        "language": "nmtran",
        "path": "./snippets/snippets.json"
      }
    ]
  },
  "semanticHighlighting": true,
  "devDependencies": {
    "@types/node": "^16.18.34",
    "@types/vscode": "^1.80.0",
    "js-yaml": "^4.1.0",
    "typescript": "^5.1.3"
  }
}
